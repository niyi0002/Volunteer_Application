<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="pane" prefHeight="700.0" prefWidth="1200.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewHistory">
    <children>
        <TableView fx:id="history" layoutX="380.0" layoutY="74.0" prefHeight="224.0" prefWidth="417.0">
            <columns>
                <TableColumn fx:id="attenedEvents" prefWidth="208.0" text="Events that attened" />
                <TableColumn fx:id="date" prefWidth="208.0" text="Date" />
            </columns>
        </TableView>
        <TableView fx:id="donationTable" layoutX="381.0" layoutY="362.0" prefHeight="212.0" prefWidth="417.0">
            <columns>
                <TableColumn fx:id="donateAmount" prefWidth="208.0" text="Donation" />
                <TableColumn fx:id="donatedDate" prefWidth="208.0" text="Date" />
            </columns>
        </TableView>
        <Label fx:id="label1" layoutX="378.0" layoutY="35.0" text="Events that volunteer has attented">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Label>
        <Label fx:id="label2" layoutX="378.0" layoutY="323.0" text="Donations that volunteer has contributed">
            <font>
                <Font name="Times New Roman" size="18.0" />
            </font>
        </Label>
        <Button fx:id="goBack" layoutX="499.0" layoutY="602.0" mnemonicParsing="false" onAction="#handleGoBack" text="Go Back">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
    </children>
    <stylesheets>
        <URL value="@design.css" />
    </stylesheets>
</AnchorPane>
