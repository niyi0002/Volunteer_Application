<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="577.0" prefWidth="321.0" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.ViewHistory">
    <children>
        <TableView fx:id="history" layoutY="40.0" prefHeight="224.0" prefWidth="320.0">
            <columns>
                <TableColumn fx:id="attenedEvents" prefWidth="154.0" text="Events that attened" />
                <TableColumn fx:id="date" prefWidth="164.0" text="Date" />
            </columns>
        </TableView>
        <TableView fx:id="donationTable" layoutY="309.0" prefHeight="212.0" prefWidth="320.0">
            <columns>
                <TableColumn fx:id="donateAmount" prefWidth="167.0" text="Donation" />
                <TableColumn fx:id="donatedDate" prefWidth="152.0" text="Date" />
            </columns>
        </TableView>
        <Label fx:id="label1" layoutX="54.0" layoutY="14.0" text="Events that volunteer has attented">
            <font>
                <Font name="Comic Sans MS" size="12.0" />
            </font>
        </Label>
        <Label fx:id="label2" layoutX="51.0" layoutY="280.0" text="Donations that volunteer has contributed">
            <font>
                <Font name="Comic Sans MS" size="12.0" />
            </font>
        </Label>
        <Button fx:id="goBack" layoutX="134.0" layoutY="527.0" mnemonicParsing="false" onAction="#handleGoBack" text="Go Back">
            <font>
                <Font name="Times New Roman" size="14.0" />
            </font>
        </Button>
    </children>
</AnchorPane>