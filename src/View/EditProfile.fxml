    <children>
        <Text layoutX="503.0" layoutY="84.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit profile">
            <font>
                <Font name="Times New Roman" size="46.0" />
            </font>
        </Text>
        <Text layoutX="503.0" layoutY="143.0" strokeType="OUTSIDE" strokeWidth="0.0" text="General account settings">
            <font>
                <Font name="Times New Roman" size="17.0" />
            </font>
        </Text>
        <TextField fx:id="lastNameField" layoutX="503.0" layoutY="235.0" prefHeight="26.0" prefWidth="167.0" promptText="Last name" />
        <TextField fx:id="addressField" layoutX="503.0" layoutY="286.0" prefHeight="26.0" prefWidth="167.0" promptText="Address" />
        <TextField fx:id="phoneNbrField" layoutX="503.0" layoutY="343.0" prefHeight="26.0" prefWidth="167.0" promptText="Phone number" />
        <Button fx:id="returnButton" layoutX="857.0" layoutY="628.0" mnemonicParsing="false" onAction="#handleReturnButton" text="Return" />
        <Button fx:id="updateButton" defaultButton="true" layoutX="55.0" layoutY="410.0" mnemonicParsing="false" onAction="#handleUpdateButton" text="Update" />
